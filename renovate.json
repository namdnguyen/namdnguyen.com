{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "packagePatterns": ["^eslint"],
      "groupName": "eslint"
    },
    {
      "packagePatterns": ["gatsby"],
      "groupName": "gatsby"
    },
    {
      "packageNames": ["react", "react-dom"],
      "groupName": "react"
    }
  ],
  "enabled": true,
  "ignoreUnstable": true,
  "labels": ["renovate"],
  "prHourlyLimit": 10,
  "prConcurrentLimit": 16,
  "rangeStrategy": "bump",
  "bumpVersion": null,
  "semanticCommits": true,
  "stabilityDays": 3,
  "schedule": "monthly",
  "timezone": "America/Chicago",
  "automerge": true,
  "major": {
      "automerge": false
  },
  "lockFileMaintenance": {
    "enabled": true
  }
}
